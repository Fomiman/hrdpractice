 */
 
function inertUser(){
	if(f.user_no.value == null){
		alert("회원번호가 입력되지 않았습니다.");
		return f.user_no.focus();
	}
	if(f.user_name.value == null){
		alert("회원명을 입렵해주세요.");
		return f.user_name.focus();
	}
	if(f.user_name.value == null){
		alert("회원명을 입렵해주세요.");
		return f.user_name.focus();
	}
	if(f.user_name.value == null){
		alert("회원명을 입렵해주세요.");
		return f.user_name.focus();
	}
	if(f.user_name.value == null){
		alert("회원명을 입렵해주세요.");
		return f.user_name.focus();
	}
	if(f.user_name.value == null){
		alert("회원명을 입렵해주세요.");
		return f.user_name.focus();
	}
	
	
}
